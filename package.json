{
  "name": "authorization-resource-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:create-migration": "npm run typeorm migration:create -- -n",
    "typeorm:generate-migration": "npm run typeorm migration:generate -- -n",
    "typeorm:run-migration": "npm run typeorm migration:run",
    "typeorm:revert-migration": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.2",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/typeorm": "^7.1.5",
    "@prisma/client": "^2.21.2",
    "cache-manager": "^3.4.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "dotenv-flow": "^3.2.0",
    "export-from-json": "^1.3.4",
    "mysql2": "2.2.5",
    "nodemailer": "^6.4.17",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "secure-password": "4.0.0",
    "slugify": "^1.4.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.2.30",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@sentry/node": "^6.0.4",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.15",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.0",
    "@types/secure-password": "3.1.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "aws-sdk": "^2.839.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "prisma": "^2.21.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
